model List<DALTW.Models.Document>

<h2>Tài liệu: @ViewBag.GradeName - @ViewBag.SemesterName</h2>

@if (Model.Count == 0)
{
    <p>Không có tài liệu nào cho lớp này.</p>
}
else
{
    <ul>
        @foreach (var doc in Model)
        {
            <li>
                <a href="/DocumentUser/Details/@doc.DocumentID">@doc.Name</a>
            </li>
        }
    </ul>
}
